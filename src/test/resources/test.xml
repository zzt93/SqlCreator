<?xml version="1.0" encoding="UTF-8" ?>

<tests xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="../../main/resources/define.xsd">

    <test>
        <updateStmt id="update"/>
    </test>

    <test>
        <query id="testOperandGeneration0">
            <select/>
            <from/>
            <where>
                <operand>
                    <udfConfig>
                        <udfList possibility="1">EQ</udfList>
                    </udfConfig>
                    <columnExpr>
                        <udfDepth>0</udfDepth>
                    </columnExpr>
                    <constExpr>
                        <udfDepth>0</udfDepth>
                    </constExpr>
                </operand>
            </where>
        </query>
        <query id="testOperandGeneration1">
            <select/>
            <from/>
            <where>
                <operand>
                    <udfConfig>
                        <udfList possibility="1">EQ</udfList>
                    </udfConfig>
                </operand>
            </where>
        </query>
        <query id="testOperandGeneration">
            <select/>
            <from/>
            <where>
                <operand>
                    <udfConfig>
                        <udfList possibility="1">AND</udfList>
                    </udfConfig>
                    <expr>
                        <udfDepth>3</udfDepth>
                    </expr>
                    <constExpr>
                        <udfConfig>
                            <udfList possibility="1">EQ</udfList>
                        </udfConfig>
                        <constExpr>
                            <udfDepth>0</udfDepth>
                        </constExpr>
                        <constExpr>
                            <udfDepth>0</udfDepth>
                        </constExpr>
                    </constExpr>
                </operand>
            </where>
        </query>
    </test>
    <!--transaction default value is false-->
    <test transaction="true" threads="3">
        <query id="intQuery" queryDepth="1" num="0">
            <select>
                <operand type="int"/>
            </select>
            <from/>
        </query>

        <query id="longQuery" queryDepth="1" num="0">
            <select>
                <operand type="long"/>
            </select>
            <from/>
        </query>

        <setOperator type="UNION">
            <operand>
                <subQuery>intQuery</subQuery>
            </operand>
            <operand>
                <subQuery>longQuery</subQuery>
            </operand>
        </setOperator>
    </test>

    <test>
        <query id="testJoin">
            <select/>
            <from>
                <join/>
            </from>
        </query>
        <query id="testJoin1">
            <select/>
            <from>
                <join>
                    <operand>
                        <tableName>test_udf</tableName>
                    </operand>
                </join>
            </from>
        </query>
        <query id="testJoin2">
            <select/>
            <from>
                <join>
                    <operand>
                        <tableName>test_udf</tableName>
                    </operand>
                    <operand>
                        <tableName>test_udf</tableName>
                    </operand>
                </join>
            </from>
        </query>
        <query id="testJoin3">
            <select/>
            <from>
                <join>
                    <operand>
                        <tableName>test_udf</tableName>
                    </operand>
                    <operand>
                        <subQuery>simpleQuery</subQuery>
                    </operand>
                    <condition/>
                </join>
            </from>
        </query>
        <query id="testJoin4">
            <select/>
            <from>
                <join>
                    <operand>
                        <tableName>test_udf</tableName>
                    </operand>
                    <operand>
                        <joinedTable/>
                    </operand>
                    <condition/>
                </join>
            </from>
        </query>
        <query id="testJoin5">
            <select/>
            <from>
                <join>
                    <operand>
                        <tableName>test_udf_2</tableName>
                    </operand>
                    <operand>
                        <tableName>test_udf</tableName>
                    </operand>
                    <condition/>
                </join>
            </from>
        </query>
    </test>

    <test>
        <query id="whereSimpleQuery" queryDepth="2" num="0">
            <select>
                <selectNum>1</selectNum>
            </select>
            <from/>
            <where/>
        </query>

        <query id="testDefaultWhere">
            <select/>
            <from/>
            <where/>
        </query>
        <query id="testWhereQuery">
            <select/>
            <from/>
            <where>
                <operand candidateQuery="whereSimpleQuery">
                    <udfConfig>
                        <udfList possibility="1">IN_QUERY</udfList>
                    </udfConfig>
                </operand>
            </where>
        </query>
        <query id="testWhereDepth">
            <select/>
            <from/>
            <where>
                <operand>
                    <udfDepth>5</udfDepth>
                </operand>
            </where>
        </query>
        <query id="testWhere">
            <select/>
            <from/>
            <where>
                <operand candidateQuery="whereSimpleQuery">
                    <columnExpr>
                        <udfDepth>3</udfDepth>
                    </columnExpr>
                    <constExpr>
                        <udfConfig>
                            <udfList possibility="1">PLUS</udfList>
                        </udfConfig>
                    </constExpr>
                </operand>
            </where>
        </query>
    </test>

    <test>
        <query id="selectQuery">
            <select>
                <operand type="int">
                    <udfConfig>
                        <udfList possibility="1">COUNT</udfList>
                    </udfConfig>
                </operand>
            </select>
            <from/>
        </query>
        <query num="100" id="testSelect0">
            <select>
                <operand type="int">
                    <columnExpr/>
                    <constExpr>
                        <udfConfig>
                            <udfList possibility="1">PLUS</udfList>
                        </udfConfig>
                    </constExpr>
                </operand>
                <operand type="long">
                    <columnExpr/>
                    <columnExpr/>
                </operand>
            </select>
            <from/>
            <groupBy/>
            <having/>
        </query>
        <query num="100" id="testSelect">
            <select>
                <operand type="int">
                    <columnExpr/>
                    <constExpr>
                        <udfConfig>
                            <udfList possibility="1">PLUS</udfList>
                        </udfConfig>
                    </constExpr>
                </operand>
                <query>selectQuery</query>
            </select>
            <from/>
            <groupBy/>
            <having/>
        </query>
    </test>

    <test>
        <query id="nonEqualJoin">
            <select/>
            <from>
                <join>
                    <operand desc="default: table or query">
                        <tableName>test_udf</tableName>
                    </operand>
                    <operand>
                        <subQuery>simpleQuery</subQuery>
                    </operand>
                    <condition>
                        <udfConfig>
                            <udfList possibility="1">AND</udfList>
                        </udfConfig>
                        <constExpr>
                            <udfConfig>
                                <udfList possibility="1">EQ</udfList>
                            </udfConfig>
                        </constExpr>
                        <expr desc="default: const/column/expr"/>
                    </condition>
                </join>
            </from>
        </query>
        <query id="simpleQuery" queryDepth="2" num="0">
            <select/>
            <from/>
            <where/>
        </query>
    </test>


</tests>
