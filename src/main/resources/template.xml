<?xml version="1.0" encoding="UTF-8" ?>

<tests xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="define.xsd">
    <test>
        <updateStmt id="update"/>
    </test>

    <!--transaction default value is false-->
    <test transaction="true" threads="3">
        <query id="intQuery" queryDepth="1" num="0">
            <select>
                <operand type="int"/>
            </select>
            <from/>
        </query>
        <query id="longQuery" queryDepth="1" num="0">
            <select>
                <operand type="long"/>
            </select>
            <from/>
        </query>
        <setOperator type="UNION">
            <operand>
                <subQuery>intQuery</subQuery>
            </operand>
            <operand>
                <subQuery>longQuery</subQuery>
            </operand>
        </setOperator>
    </test>

    <test>
        <query id="testJoin">
            <select/>
            <from>
                <join/>
            </from>
        </query>
        <query id="testJoin1">
            <select/>
            <from>
                <join>
                    <operand>
                        <tableName>test_udf</tableName>
                    </operand>
                </join>
            </from>
        </query>
        <query id="testJoin2">
            <select/>
            <from>
                <join>
                    <operand>
                        <tableName>test_udf</tableName>
                    </operand>
                    <operand>
                        <tableName>test_udf</tableName>
                    </operand>
                </join>
            </from>
        </query>
        <query id="testJoin3">
            <select/>
            <from>
                <join>
                    <operand>
                        <tableName>test_udf</tableName>
                    </operand>
                    <operand>
                        <subQuery>simpleQuery</subQuery>
                    </operand>
                    <condition/>
                </join>
            </from>
        </query>
    </test>

    <test>
        <query id="testWhere">
            <select/>
            <from/>
            <where/>
        </query>
    </test>

    <test>
        <query id="nonEqualJoin">
            <select/>
            <from>
                <join>
                    <operand desc="default: table or query">
                        <tableName>test_udf</tableName>
                    </operand>
                    <operand>
                        <subQuery>simpleQuery</subQuery>
                    </operand>
                    <condition udfDepth="1">
                        <udfConfig>
                            <udfList possibility="1">AND</udfList>
                        </udfConfig>
                        <const udfDepth="1">
                            <udfConfig>
                                <udfList possibility="1">EQ</udfList>
                            </udfConfig>
                        </const>
                        <expr desc="default: const/column/expr"/>
                    </condition>
                </join>
            </from>
        </query>
        <query id="simpleQuery" queryDepth="2" num="0">
            <select/>
            <from/>
            <where/>
        </query>
    </test>

    <test>
        <query num="1000" id="test">
            <select>
                <operand type="int">
                    <column udfDepth="1"/>
                    <const udfDepth="3">
                        <udfConfig>
                            <udfList possibility="1">PLUS</udfList>
                        </udfConfig>
                    </const>
                </operand>
                <query>simpleQuery</query>
            </select>
            <from/>
            <groupBy/>
            <having/>
        </query>
    </test>

</tests>
