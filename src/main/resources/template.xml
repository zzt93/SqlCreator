<?xml version="1.0" encoding="UTF-8" ?>

<tests xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="define.xsd">
    <test>
        <query id="nonEqualJoin">
            <from>
                <join>
                    <operand desc="default: table or query"/>
                    <operand>
                        <subQuery ref="simpleQuery"/>
                    </operand>
                    <condition udfDepth="1">
                        <udfConfig>
                            <udfList list="and">1</udfList>
                        </udfConfig>
                        <const udfDepth="1">
                            <udfConfig>
                                <udfList list="=">1</udfList>
                            </udfConfig>
                        </const>
                        <expr desc="default: const/expr/operand"/>
                    </condition>
                </join>
            </from>
        </query>
        <updateStmt id="update"/>
        <query id="simpleQuery" queryDepth="2"/>
    </test>


    <test>
        <query num="1000" id="test">
            <!--specify needed keyword-->
            <groupBy/>
            <having/>
        </query>
    </test>


    <!--transaction default value is false-->
    <test transaction="true" threads="3">
    </test>





</tests>
