       xsi:noNamespaceSchemaLocation="define.xsd">
    <test>
        <query id="nonEqualJoin">
            <from>
                <join>
                    <operand desc="default: table or query"/>
                    <operand>
                        <subQuery ref="simpleQuery"/>
                    </operand>
                    <condition desc="condition" udfCentain="and">
                        <expr udfCentain="=" udfDepth="1"/>
                        <expr desc="default: const/expr/operand"/>
                    </condition>
                </join>
            </from>
        </query>
        <query id="simpleQuery" depth="2"/>
    </test>


    <test>
        <query num="1000" id="test">
            <!--specify needed keyword-->
            <group-by/>
            <having/>
        </query>
    </test>


    <!--transaction default value is false-->
    <test transaction="true" threads="3">
    </test>





