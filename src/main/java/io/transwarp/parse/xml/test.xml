<?xml version="1.0" encoding="UTF-8" ?>

<tests xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="define.xsd">
    <test>
        <select table="src.sql" dialect="oracle" name="">
            <!--<from/> means default: table and subQuery-->
            <where type="=;>="/>
            <join>
                <operand>
                    <!--<table/> means default: -->
                    <sub-query depth="5" ref="simpleQuery"/>
                </operand>
                <condition type=""/>
            </join>
        </select>
    </test>


    <test>
        <sql type="select" num="1000">
            <!--specify needed keyword-->
            <group-by/>
            <having/>
        </sql>
    </test>


    <!--transaction default value is false-->
    <test transaction="true" threads="3">
        <sql type="select" num="1000"/>
        <sql type="update" num="1000"/>
        <sql type="delete" num="1000"/>
    </test>


    <test>
        <sql type="select">
            <join>
                <condition>=</condition>
            </join>
        </sql>
    </test>


</tests>
