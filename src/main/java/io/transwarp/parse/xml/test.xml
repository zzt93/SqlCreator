<?xml version="1.0" encoding="UTF-8" ?>

<tests xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="define.xsd">
    <test>
        <query id="nonEqualJoin">
            <from>
                <join>
                    <operand desc="default: table or query"/>
                    <operand>
                        <query depth="2" ref="simpleQuery"/>
                    </operand>
                    <operand desc="condition" udf="and">
                        <expr udf="=" udf-depth="1"/>
                        <operand desc="default: const/expr/operand"/>
                    </operand>
                </join>
            </from>
        </query>
        <query id="simpleQuery"/>
    </test>


    <test>
        <query num="1000">
            <!--specify needed keyword-->
            <group-by/>
            <having/>
        </query>
    </test>


    <!--transaction default value is false-->
    <test transaction="true" threads="3">
    </test>





</tests>
